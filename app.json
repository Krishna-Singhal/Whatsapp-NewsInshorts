{
    "name": "Whatsbot-NewsInshort",
    "description": "Automated News Whatsapp Bot",
    "repository": "https://github.com/Krishna-Singhal/Whatsapp-NewsInshorts",
    "keywords": [
      "whatsapp-news",
      "whatsapp-newsbot",
      "whatsapp-bot",
      "newsinshort"
    ],
    "stack": "container",
    "env": {
      "SESSION": {
        "description": "https://github.com/tuhinpal/WhatsBot/wiki/Deploy-with-Heroku",
        "value": "",
        "required": "true"
      },
      "CHAT_ID": {
        "description": "Chat Id where bot will send news",
        "value": "",
        "required": "true"
      },
      "CATEGORY": {
        "description": "Category of news, defaults to all",
        "value": "all"
      },
      "DELAY": {
        "description": "Delay of news automation in seconds, defaults to 150 seconds",
        "value": ""
      }
    }
  }